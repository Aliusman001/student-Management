                <section class="d-flex"><h6  class="text-darks">Name:</h6><h6 class="text-dark margin"><%=data.Name%></h6></section>
                <section class="d-flex"><h6  class="text-darks">Roll Number:</h6><h6 class="text-dark margin"><%=data.RollNumber%></h6></section>
                <section class="d-flex"><h6  class="text-darks">Course:</h6><h6 class="text-dark margin"><%=data.Courses%></h6></section>
            </section>
            <section class="col-6">
                <section class="d-flex"><h6  class="text-darks">Gender:</h6><h6 class="text-dark margin"><%=data.Gender%></h6></section>
                <section class="d-flex"><h6  class="text-darks">Semester:</h6><h6 class="text-dark margin"><%=data.Semester%></h6></section>

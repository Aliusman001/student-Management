                <th scope="col">Obtained Marks</th>
                <th scope="col">Grade</th>
                <th scope="col">Presentage</th>
            </tr>
        </thead>
        <tbody>
            <tr class="text-dark">
                <td scope="row">1</td>
                <td><%=data.SubjectOne%></td>
                <td><%=data.Subject1Marks%></td>
                <td><%=data.Subject1ObtainedMarks%></td>
                <td><%=data.Subject1Grade%></td>
                <td><%=data.Subject1Persentage%></td>
            </tr>
            <tr class="text-dark">
                <td scope="row">2</td>
                <td><%=data.Subject2%></td>
                <td><%=data.Subject2Marks%></td>
                <td><%=data.Subject2ObtainedMarks%></td>
                <td><%=data.Subject2Grade%></td>
                <td><%=data.Subject2Persentage%></td>
            </tr>
            <tr class="text-dark">
                <td scope="row">3</td>
                <td><%=data.Subject3%></td>
                <td><%=data.Subject3Marks%></td>
                <td><%=data.Subject3ObtainedMarks%></td>
                <td><%=data.Subject3Grade%></td>
                <td><%=data.Subject3Persentage%></td>
            </tr>
            <tr class="text-dark">
                <td scope="row">4</td>

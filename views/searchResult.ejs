<section class="hide">
    <% if(user==="admin@gmail.com"){ %>
        <%-include("partials/header");-%>
    <% }else{ %>
        <%-include("partials/student_header");-%>
    <% } %>
</section>
<h3 class='text-white mt-3 mb-3 an text-center'>Search Result</h3>
<section class=" hide">
    <form action="searchResult" method="post">
  <section class="row mt-3">
    <section class="col-2">
        <label for="semester">Semester</label>
        <select name="semester" class="form-select">
            <option selected>Select Semester</option>
            <option value="I">I</option>
            <option value="II">II</option>
            <option value="III">III</option>
            <option value="IV">IV</option>
            <option value="V">V</option>
            <option value="VI">VI</option>
            <option value="VII">VII</option>
            <option value="VIII">VIII</option>
        </select>
    </section>
    <section class="form-group col-3">
        <label for="session">Session</label><br>
        <select name="session" id="session" class="form-select">
                <option selected>Select Session</option>
            <option value="2019-2023">2019-2023</option>
            <option value="2020-2024">2020-2024</option>
            </select>
    </section>

   <section class="col-3">
    <section class="form-group d-flex"style='margin-top: 32px;'>
        <input type="text" name="search" class="form-control bor">
        <button class="btn btn-primary  borbutton">Search</button>
       </section>
   </section>
  </section>
    </form>
</section>
<%if(data!==null){ %>

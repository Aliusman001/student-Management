<% if(information[0].UserName=="admin@gmail.com"){ %>
    <%-include("partials/header");-%>
<% }else{ %>
    <%-include("partials/student_header");-%>
<% } %>




    <section class="conatiner text-center ">
        <h4 class="text-white mt-3 mb-3 an">Account Information</h4>
        <section class="conatiners slide">
            <form action="/account" method="post" enctype="multipart/form-data">
                <input type="file" id="fileselect" name="image" style="display: none;">
                <input type="hidden" name="newusername" value="<%=information[0].UserName%>">
            </form>

            <img src="<%=information[0].Profile%>" class="profile" alt="">
             <button id="button" class="profileImage"><i class="fas fa-camera"></i></button>

            <p class="mar">
                <%=information[0].UserName%>
            </p>
            <% if(information[0].UserName=="admin@gmail.com"){ %>
                <p class="mar">Head of the Organization</p>
                <p class="mar">
                    <%=information[0].Gender%>
                </p>
            <% }else{ %>
                <p class="mar">
                    <%=information[0].Gender%>
                </p>
                <p class="mar">student</p>
            <% } %>
           
         
           
        </section>
    </section>





    <%-include("partials/footer");-%>